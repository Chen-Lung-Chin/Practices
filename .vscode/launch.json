    "version": "0.2.0",
    "configurations": [
    {
        "name": "Debug: read_json_file",
        "type": "go",
        "request": "launch",
        "mode": "exec",
        "program": "${workspaceFolder}/Go/demo/bin/read_json_file"
    },
        {
            "name": "Golan: leave server",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "args": ["run"],
            "program": "${workspaceFolder}\\Go\\leaf\\leafserver\\bin\\server.exe"
        },
        {
            "name": "Golan: Current File",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "args": ["run"],
            "program": "${file}"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\Django\\generic_view\\manage.py",
            "args": ["runserver", "8080", "--noreload"],
            "django": true
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9000
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000
        }
    ]
}