        "functional": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "sqlite3.c": "cpp",
        "deflate.c": "cpp",
        "inflate.c": "cpp",
        "trees.c": "cpp",
        "bitset": "cpp",
        "__locale": "cpp",
        "string": "cpp",
        "__bit_reference": "cpp",
        "__tree": "cpp",
        "algorithm": "cpp",
        "map": "cpp",
        "set": "cpp",
        "adler32.c": "cpp",
        "cmakeccompilerid.c": "cpp",
        "atomic": "cpp",
        "locale": "cpp",
        "memory": "cpp",
